<!DOCTYPE html>
<html class="no-js">

<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <title>
    {{ docs.page.meta.title }}
  </title>
  <meta name="description" content="{{ docs.page.meta.description }}">
  <meta name="keywords" content="{{ docs.page.meta.keywords }}">
  <meta name="author" content="{{ docs.page.meta.author }}">
  <meta name="viewport" content="width=device-width">


  <meta property="og:title" content="{{ docs.page.meta.title }}" />
  <meta property="og:url" content="http://admataz.com{{route}}/" />
  <meta property="og:description" content="{{ docs.page.meta.description }}" />
  <meta property="og:site_name" content="admataz. Web apps and bespoke web development, London UK" />
  <meta property="og:image" content="http://admataz.com/assets/images/admataz_front.png" />


  <link rel="alternate" title="admataz. Web apps and bespoke web development, London UK" type="application/json"
    href="http://admataz.com/feed.json" />
  <link rel="alternate" type="application/atom+xml" title="Blog" href="/feed.xml?type=blog" />

  <link rel="stylesheet" href="/main.css">
  <script src="//use.typekit.net/ubc1oxm.js"></script>
  <script>try { Typekit.load(); } catch (e) { }</script>
  {{#each styles}}
  <link rel="stylesheet" href="{{this}}" />
  {{/each}}
</head>

<body>
  <header>
    <div class="svg-logo">
      <a href="/">
        <img src="/assets/images/admataz-logo_bak.svg">
      </a>
    </div>
  </header>


  {{{ docs.headerContent.html }}}

  {{{ pageContent }}}

  {{{ docs.footerContent.html }}}

  <script src="/js/components.js"></script>

  {{#each scripts}}
  <script src="{{this}}"></script>
  {{/each}}
  <script>
    (function (i, s, o, g, r, a, m) {
      i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
        (i[r].q = i[r].q || []).push(arguments)
      }, i[r].l = 1 * new Date(); a = s.createElement(o),
        m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
    })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');
    ga('create', 'UA-310888-1', 'auto');
    ga('send', 'pageview');
  </script>

</body>

</html>